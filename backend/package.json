{
  "name": "techhub-backend",
  "version": "1.0.0",
  "description": "Backend services for TechHub News RSS scraping and automation",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "scrape": "node services/rssScraperService.js",
    "ticker": "node services/tickerUpdateService.js",
    "dev": "nodemon server.js",
    "dev:scrape": "nodemon services/rssScraperService.js",
    "cron": "node server.js",
    "cron:rss": "node server.js run rss",
    "cron:quick": "node server.js run quick",
    "cron:cleanup": "node server.js run cleanup",
    "cron:stats": "node server.js run stats",
    "test": "jest",
    "lint": "eslint services/**/*.js"
  },
  "keywords": [
    "rss",
    "scraper",
    "news",
    "automation"
  ],
  "author": "TechHub Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "rss-parser": "^3.13.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}